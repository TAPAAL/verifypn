<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
    <net id="ComposedModel" type="http://www.pnml.org/version-2009/grammar/ptnet">
        <name>
            <text>ComposedModel</text>
        </name>
        <declaration>
            <structure>
                <declarations>
                    <namedsort id="dot" name="dot">
                        <dot/>
                    </namedsort>
                    <namedsort id="abc" name="abc">
                        <cyclicenumeration>
                            <feconstant id="a" name="abc"/>
                            <feconstant id="b" name="abc"/>
                            <feconstant id="c" name="abc"/>
                        </cyclicenumeration>
                    </namedsort>
                    <variabledecl id="x" name="x">
                        <usersort declaration="abc"/>
                    </variabledecl>
                </declarations>
            </structure>
        </declaration>
        <page id="page0">
            <place id="P0">
                <name>
                    <text>P0</text>
                    <graphics>
                        <offset x="0" y="0"/>
                    </graphics>
                </name>
                <graphics>
                    <position x="390" y="420"/>
                </graphics>
                <type>
                    <text>abc</text>
                    <structure>
                        <usersort declaration="abc"/>
                    </structure>
                </type>
                <hlinitialMarking>
                    <text>3'a</text>
                    <structure>
                        <add>
                            <subterm>
                                <numberof>
                                    <subterm>
                                        <numberconstant value="3">
                                            <positive/>
                                        </numberconstant>
                                    </subterm>
                                    <subterm>
                                        <useroperator declaration="a"/>
                                    </subterm>
                                </numberof>
                            </subterm>
                        </add>
                    </structure>
                </hlinitialMarking>
            </place>
            <transition distribution="constant" id="T1" value="1.0">
                <name>
                    <graphics>
                        <offset x="0" y="0"/>
                    </graphics>
                    <text>T1</text>
                </name>
                <graphics>
                    <position x="540" y="420"/>
                </graphics>
            </transition>
            <arc id="P0_to_T1" source="P0" target="T1" type="normal">
                <hlinscription>
                    <text>3'a - 1'x</text>
                    <structure>
                        <subtract>
                            <subterm>
                                <numberof>
                                    <subterm>
                                        <numberconstant value="3">
                                            <positive/>
                                        </numberconstant>
                                    </subterm>
                                    <subterm>
                                        <useroperator declaration="a"/>
                                    </subterm>
                                </numberof>
                            </subterm>
                            <subterm>
                                <numberof>
                                    <subterm>
                                        <numberconstant value="1">
                                            <positive/>
                                        </numberconstant>
                                    </subterm>
                                    <subterm>
                                        <variable refvariable="x"/>
                                    </subterm>
                                </numberof>
                            </subterm>
                        </subtract>
                    </structure>
                </hlinscription>
            </arc>
        </page>
    </net>
</pnml>
